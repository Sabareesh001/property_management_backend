  users_parking_pricing_created_byTousers users?                         @relation("parking_pricing_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users_parking_pricing_deleted_byTousers users?                         @relation("parking_pricing_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
