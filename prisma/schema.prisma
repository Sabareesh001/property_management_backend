model property_unit_amenities {
  id               String         @id @db.Uuid @default(uuid())
  property_unit_id String         @db.Uuid
  amenity_id       String         @db.Uuid
  deleted_at       DateTime?      @db.Timestamp(6)
  deleted_by       String?        @db.Uuid
  created_by       String?        @db.Uuid
  created_at       DateTime?      @db.Timestamp(6)
  is_active        Boolean?       @default(true)
  amenities        amenities      @relation(fields: [amenity_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  property_units   property_units @relation(fields: [property_unit_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model property_unit_utilities {
  id               String         @unique @db.Uuid @default(uuid())
  property_unit_id String         @db.Uuid
  utility_id       String         @db.Uuid
  deleted_at       DateTime?      @db.Timestamp(6)
  deleted_by       String?        @db.Uuid
  created_by       String?        @db.Uuid
  created_at       DateTime?      @db.Timestamp(6)
  is_active        Boolean?       @default(true)
  property_units   property_units @relation(fields: [property_unit_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  utilities        utilities      @relation(fields: [utility_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model property_units {
  id                                     String                        @id @db.Uuid @default(uuid())
  name                                   String?                       @db.VarChar
  unit_id                                String?                       @db.VarChar
  address                                String?                       @db.VarChar
  company                                String                        @db.Uuid
  beds                                   Int?
  baths                                  Int?
  bhk                                    Int?
  area_in_sqft                           Decimal?                      @db.Decimal(10, 2)
  price_in_aed                           Decimal?                      @db.Decimal(10, 2)
  main_image                             String?                       @db.VarChar
  image_list                             String?                       @db.VarChar
  primary_pricing_id                     String                        @unique @db.Uuid
  secondary_pricing_id                   String?                       @db.Uuid
  one_time_cost_pricing_id               String?                       @db.Uuid
  refundables_pricing_id                 String?                       @db.Uuid
  inventory_pricing_id                   String?                       @db.Uuid
  parking_pricing_id                     String?                       @db.Uuid
  is_deleted                             Boolean?
  deleted_at                             DateTime?                     @db.Timestamp(6)
  deleted_by                             String?                       @db.Uuid
  created_by                             String?                       @db.Uuid
  created_at                             DateTime?                     @db.Timestamp(6)
  is_active                              Boolean?                      @default(true)
  property_unit_amenities                property_unit_amenities[]
  property_unit_utilities                property_unit_utilities[]
  companies                              companies                     @relation(fields: [company], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users_property_units_created_byTousers users?                        @relation("property_units_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users_property_units_deleted_byTousers users?                        @relation("property_units_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  inventory_pricing                      inventory_pricing?            @relation(fields: [inventory_pricing_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  one_time_cost_pricing                  one_time_cost_pricing?        @relation(fields: [one_time_cost_pricing_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  parking_pricing                        parking_pricing?              @relation(fields: [parking_pricing_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  primary_pricing                        primary_pricing               @relation(fields: [primary_pricing_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  refundables_pricing                    refundables_pricing?          @relation(fields: [refundables_pricing_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  secondary_pricing                      secondary_pricing?            @relation(fields: [secondary_pricing_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  quotation_quoted_properties            quotation_quoted_properties[]
  quoted_property_units                  quoted_property_units?
}

model quotation_quoted_properties {
  id                    String                @id @db.Uuid @default(uuid())
  quoted_property_id    String                @db.Uuid
  original_property_id  String                @db.Uuid
  quotation_id          String                @db.Uuid
  deleted_at            DateTime?             @db.Timestamp(6)
  deleted_by            String?               @db.Uuid
  created_by            String?               @db.Uuid
  created_at            DateTime?             @db.Timestamp(6)
  is_active             Boolean?              @default(true)
  property_units        property_units        @relation(fields: [original_property_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  quotations            quotations            @relation(fields: [quotation_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  quoted_property_units quoted_property_units @relation(fields: [quoted_property_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model quotations {
  id                          String                        @id @db.Uuid @default(uuid())
  user_id                     String                        @unique @db.Uuid
  grace_period                Int?
  lease_start_date            DateTime?                     @db.Timestamp(6)
  lease_end_date              DateTime?                     @db.Timestamp(6)
  rent_start_date             DateTime?                     @db.Timestamp(6)
  deleted_at                  DateTime?                     @db.Timestamp(6)
  deleted_by                  String?                       @db.Uuid
  created_by                  String?                       @db.Uuid
  created_at                  DateTime?                     @db.Timestamp(6)
  is_active                   Boolean?                      @default(true)
  quotation_quoted_properties quotation_quoted_properties[]
  users                       users                         @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

