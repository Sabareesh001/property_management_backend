generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model admins {
  id          Int       @id @default(autoincrement())
  user_id     String    @unique @db.Uuid
  deleted_at  DateTime? @db.Timestamp(6)
  deleted_by  String?   @db.Uuid
  created_by  String?   @db.Uuid
  is_customer Boolean?
  created_at  DateTime? @db.Timestamp(6)
  is_active   Boolean?  @default(true)
  users       users     @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model amenities {
  id                             String                           @id @db.Uuid @default(uuid())
  name                           String?                          @db.VarChar
  price                          Decimal?                         @db.Decimal(10, 2)
  image                          String?                          @db.VarChar
  is_free                        Boolean?
  start_date                     DateTime?                        @db.Timestamp(6)
  end_date                       DateTime?                        @db.Timestamp(6)
  deleted_at                     DateTime?                        @db.Timestamp(6)
  deleted_by                     String?                          @db.Uuid
  created_by                     String?                          @db.Uuid
  created_at                     DateTime?                        @db.Timestamp(6)
  is_active                      Boolean?                         @default(true)
  property_unit_amenities        property_unit_amenities[]
  quoted_property_unit_amenities quoted_property_unit_amenities[]
}

model companies {
  id             String           @id @db.Uuid @default(uuid())
  name           String?          @db.VarChar
  deleted_at     DateTime?        @db.Timestamp(6)
  deleted_by     String?          @db.Uuid
  created_by     String?          @db.Uuid
  created_at     DateTime?        @db.Timestamp(6)
  is_active      Boolean?         @default(true)
  property_units property_units[]
}

model inventory_pricing {
  id                                        String                         @id @db.Uuid @default(uuid())
  revenue_type                              Int
  pricing_component                         Int
  tax_group                                 Int
  pricing_based_on                          Int
  item_unit_price                           Decimal?                       @db.Decimal(10, 2)
  quantity                                  Int?
  discount                                  Decimal?                       @db.Decimal(10, 2)
  deleted_at                                DateTime?                      @db.Timestamp(6)
  deleted_by                                String?                        @db.Uuid
  created_by                                String?                        @db.Uuid
  created_at                                DateTime?                      @db.Timestamp(6)
  is_active                                 Boolean?                       @default(true)
  users_inventory_pricing_created_byTousers users?                         @relation("inventory_pricing_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users_inventory_pricing_deleted_byTousers users?                         @relation("inventory_pricing_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  master_pricing_based_on                   master_pricing_based_on        @relation(fields: [pricing_based_on], references: [id], onDelete: NoAction, onUpdate: NoAction)
  master_pricing_component_types            master_pricing_component_types @relation(fields: [pricing_component], references: [id], onDelete: NoAction, onUpdate: NoAction)
  master_revenue_types                      master_revenue_types           @relation(fields: [revenue_type], references: [id], onDelete: NoAction, onUpdate: NoAction)
  master_tax_groups                         master_tax_groups              @relation(fields: [tax_group], references: [id], onDelete: NoAction, onUpdate: NoAction)
  property_units                            property_units[]
}

model master_components_based_ons {
  id                           Int                            @id @default(autoincrement()) 
  name                         String?                        @db.VarChar
  is_active                    Boolean?                       @default(true)
  one_time_cost_pricing        one_time_cost_pricing[]
  parking_pricing              parking_pricing[]
  primary_pricing              primary_pricing[]
  quoted_one_time_cost_pricing quoted_one_time_cost_pricing[]
  quoted_parking_pricing       quoted_parking_pricing[]
  quoted_primary_pricing       quoted_primary_pricing[]
  quoted_refundables_pricing   quoted_refundables_pricing[]
  quoted_secondary_pricing     quoted_secondary_pricing[]
  refundables_pricing          refundables_pricing[]
  secondary_pricing            secondary_pricing[]
}

model master_pricing_based_on {
  id                       Int                        @id @default(autoincrement())
  name                     String?                    @db.VarChar
  is_active                Boolean?                   @default(true)
  inventory_pricing        inventory_pricing[]
  quoted_inventory_pricing quoted_inventory_pricing[]
}

model master_pricing_component_types {
  id                           Int                            @id @default(autoincrement())
  name                         String?                        @db.VarChar
  is_active                    Boolean?                       @default(true)
  inventory_pricing            inventory_pricing[]
  one_time_cost_pricing        one_time_cost_pricing[]
  parking_pricing              parking_pricing[]
  primary_pricing              primary_pricing[]
  quoted_inventory_pricing     quoted_inventory_pricing[]
  quoted_one_time_cost_pricing quoted_one_time_cost_pricing[]
  quoted_parking_pricing       quoted_parking_pricing[]
  quoted_primary_pricing       quoted_primary_pricing[]
  quoted_refundables_pricing   quoted_refundables_pricing[]
  quoted_secondary_pricing     quoted_secondary_pricing[]
  refundables_pricing          refundables_pricing[]
  secondary_pricing            secondary_pricing[]
}

model master_revenue_types {
  id                           Int                            @id @default(autoincrement())
  name                         String?                        @db.VarChar
  is_active                    Boolean?                       @default(true)
  inventory_pricing            inventory_pricing[]
  one_time_cost_pricing        one_time_cost_pricing[]
  parking_pricing              parking_pricing[]
  primary_pricing              primary_pricing[]
  quoted_inventory_pricing     quoted_inventory_pricing[]
  quoted_one_time_cost_pricing quoted_one_time_cost_pricing[]
  quoted_parking_pricing       quoted_parking_pricing[]
  quoted_primary_pricing       quoted_primary_pricing[]
  quoted_refundables_pricing   quoted_refundables_pricing[]
  quoted_secondary_pricing     quoted_secondary_pricing[]
  refundables_pricing          refundables_pricing[]
  secondary_pricing            secondary_pricing[]
}

model master_tax_groups {
  id                           Int                            @id @default(autoincrement())
  name                         String?                        @db.VarChar
  is_active                    Boolean?                       @default(true)
  inventory_pricing            inventory_pricing[]
  one_time_cost_pricing        one_time_cost_pricing[]
  parking_pricing              parking_pricing[]
  primary_pricing              primary_pricing[]
  quoted_inventory_pricing     quoted_inventory_pricing[]
  quoted_one_time_cost_pricing quoted_one_time_cost_pricing[]
  quoted_parking_pricing       quoted_parking_pricing[]
  quoted_primary_pricing       quoted_primary_pricing[]
  quoted_refundables_pricing   quoted_refundables_pricing[]
  quoted_secondary_pricing     quoted_secondary_pricing[]
  refundables_pricing          refundables_pricing[]
  secondary_pricing            secondary_pricing[]
}

model one_time_cost_pricing {
  id                                            String                         @id @db.Uuid @default(uuid())
  revenue_type                                  Int
  pricing_component                             Int
  tax_group                                     Int
  component_based_on                            Int
  uom_value                                     Decimal?                       @db.Decimal(10, 2)
  discount                                      Decimal?                       @db.Decimal(10, 2)
  deleted_at                                    DateTime?                      @db.Timestamp(6)
  deleted_by                                    String?                        @db.Uuid
  created_by                                    String?                        @db.Uuid
  created_at                                    DateTime?                      @db.Timestamp(6)
  is_active                                     Boolean?                       @default(true)
  master_components_based_ons                   master_components_based_ons    @relation(fields: [component_based_on], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users_one_time_cost_pricing_created_byTousers users?                         @relation("one_time_cost_pricing_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users_one_time_cost_pricing_deleted_byTousers users?                         @relation("one_time_cost_pricing_deleted_byTousers", fields: [deleted_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  master_pricing_component_types                master_pricing_component_types @relation(fields: [pricing_component], references: [id], onDelete: NoAction, onUpdate: NoAction)
  master_revenue_types                          master_revenue_types           @relation(fields: [revenue_type], references: [id], onDelete: NoAction, onUpdate: NoAction)
  master_tax_groups                             master_tax_groups              @relation(fields: [tax_group], references: [id], onDelete: NoAction, onUpdate: NoAction)
  property_units                                property_units[]
}

